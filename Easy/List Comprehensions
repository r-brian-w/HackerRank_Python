if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    results = []
    for x in [n]:
       for y in [n]:
            for z in [n]:
                n >= 3
                x + y + z != n
                results.append([x, y, z])
    
    print(results)
    
